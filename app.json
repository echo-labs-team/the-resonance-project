{
  "expo": {
    "name": "Echo.Church",
    "slug": "echo",
    "scheme": "echo",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["./assets/"],
    "ios": {
      "userInterfaceStyle": "dark",
      "supportsTablet": false,
      "bundleIdentifier": "com.echo.church.app"
    },
    "isDetached": true,
    "detach": {
      "iosExpoViewUrl": "https://s3.amazonaws.com/exp-exponent-view-code/ios-v2.14.3-sdk36.0.0-00f587c0-b8c9-4b6b-88f6-2ccd4f20dc9c.tar.gz",
      "androidExpoViewUrl": "https://s3.amazonaws.com/exp-exponent-view-code/android-v2.14.0-sdk36.0.0-fb8689e6-fd42-4b18-b01f-ac2ef3bc8681.tar.gz"
    },
    "android": {
      "userInterfaceStyle": "dark",
      "package": "com.echo.church.app"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
