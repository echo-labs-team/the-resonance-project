    "clear": ". scripts/Clear.sh"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.3",
    "axios": "^0.19.0",
    "expo": "^35.0.0",
    "expo-asset": "^7.0.0",
    "expo-blur": "^7.0.0",
    "expo-font": "^7.0.0",
    "expo-web-browser": "^7.0.0",
    "expokit": "^35.0.3",
    "react": "^16.10.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-dropdownalert": "^4.1.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-modal": "^11.1.0",
    "react-native-reanimated": "^1.1.0",
    "react-native-svg": "^9.9.9",
    "react-native-unimodules": "^0.6.0",
    "react-native-youtube": "^2.0.0-beta",
    "react-navigation": "^4.0.3",
    "react-navigation-drawer": "^2.1.2",
    "react-navigation-stack": "^1.5.5",
    "react-navigation-tabs": "^2.4.2",
    "rn-placeholder": "^3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^7.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-flowtype": "^3.9.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^3.8.0",
    "flow-bin": "^0.104.0",
    "flow-typed": "^2.6.1",
    "husky": "^3.0.0",
    "jest-expo": "^35.0.0",
    "lint-staged": "^9.1.0",
    "prettier": "^1.17.1",
    "react-devtools": "^3.6.1"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "flow focus-check",
      "eslint",
      "git add"
    ]
  },
  "resolutions": {
    "react-native-svg": "9.4.0"
  }
}
