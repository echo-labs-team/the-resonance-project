{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "build-android": "expo build:android -t app-bundle --release-channel production",
    "build-ios": "expo build:ios -t archive --release-channel production",
    "build-storybook": "build-storybook",
    "format": "prettier --write \"**/*.js\"",
    "ios": "expo start --ios",
    "lint": "eslint .",
    "prestorybook": "rnstl",
    "publish:history": "expo publish:history",
    "publish": "npx expo-optimize . && expo publish",
    "start": "expo start",
    "storybook": "start-storybook -p 7007",
    "test:watch": "jest --watchAll",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "expo": "^44.0.0",
    "expo-analytics-amplitude": "~11.1.0",
    "expo-application": "~4.0.1",
    "expo-blur": "~11.0.0",
    "expo-constants": "~13.0.1",
    "expo-device": "~4.1.0",
    "expo-font": "~10.0.4",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.6",
    "expo-web-browser": "~10.1.0",
    "fast-html-parser": "^1.0.1",
    "formik": "^2.2.9",
    "html-entities": "^2.3.2",
    "react": "17.0.1",
    "react-content-loader": "^6.0.3",
    "react-dom": "17.0.1",
    "react-error-boundary": "^3.1.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-44.0.0.tar.gz",
    "react-native-dropdownalert": "^4.3.0",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-hyperlink": "^0.0.19",
    "react-native-modal": "^12.0.2",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "react-native-webview": "11.15.0",
    "react-query": "^3.18.1",
    "redaxios": "^0.4.1",
    "sentry-expo": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/react-native": "^7.2.0",
    "@types/amplitude-js": "^8.0.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "expo-cli": "^4.8.1",
    "husky": "^7.0.1",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "react-native-storybook-loader": "^2.0.4",
    "typescript": "~4.3.5"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|@sentry/.*|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./components"
      ],
      "pattern": "**/*.stories.js"
    }
  },
  "private": true
}
