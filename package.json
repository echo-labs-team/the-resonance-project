{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "tunnel": "expo start --tunnel",
    "eject": "expo eject",
    "postinstall": "flow-typed install",
    "publish": "expo optimize && expo publish --release-channel=development",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll=false",
    "test:watch": "node ./node_modules/jest/bin/jest.js --watchAll",
    "lint": "eslint .",
    "flow": "flow",
    "format": "prettier --write \"**/*.js\"",
