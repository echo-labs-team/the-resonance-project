{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "tunnel": "expo start --tunnel",
    "eject": "expo eject",
    "postinstall": "flow-typed install",
    "publish": "expo optimize && expo publish --release-channel=default",
    "test": "node ./node_modules/jest-expo/bin/jest.js --watchAll=false",
    "test:watch": "node ./node_modules/jest-expo/bin/jest.js --watchAll",
    "lint": "eslint .",
    "flow": "flow",
    "format": "prettier --write \"**/*.js\"",
    "react-devtools": "react-devtools",
    "clear": ". scripts/Clear.sh",
    "nuke": "rm -rf node_modules/ && rm -f package-lock.json && npm cache verify && npm install && expo r -c"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "axios": "^0.19.0",
    "expo": "^36.0.0",
    "expo-analytics-amplitude": "~8.0.0",
    "expo-asset": "~8.0.0",
    "expo-blur": "~8.0.0",
    "expo-font": "~8.0.0",
    "expo-web-browser": "~8.0.0",
    "expokit": "^36.0.0",
    "react": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-dropdownalert": "^4.1.0",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-modal": "^11.1.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-svg": "9.13.3",
    "react-native-unimodules": "0.7.0",
    "react-native-youtube": "^2.0.0",
    "react-navigation": "^4.0.3",
    "react-navigation-drawer": "^2.1.2",
    "react-navigation-stack": "^1.5.5",
    "react-navigation-tabs": "^2.4.2",
    "rn-placeholder": "^3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^3.11.7",
    "flow-bin": "^0.112.0",
    "flow-typed": "^2.6.2",
    "husky": "^3.0.0",
    "jest-expo": "^36.0.0",
    "lint-staged": "^9.1.0",
    "prettier": "^1.17.1",
    "react-devtools": "^4.2.1",
    "react-native-testing-library": "^1.12.0",
    "react-test-renderer": "^16.12.0"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "flow focus-check",
      "eslint",
      "git add"
    ]
  }
}
